{
    "name": "rst-compiler",
    "type": "module",
    "version": "0.5.7",
    "description": "Parses reStructuredText and generates HTML or VitePress-compatible Markdown",
    "license": "MIT",
    "private": false,
    "sideEffects": false,
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.umd.cjs"
        }
    },
    "files": [
        "README.md",
        "tsconfig.json",
        "dist/*",
        "src/*"
    ],
    "homepage": "https://trinovantes.github.io/rst-compiler/",
    "repository": {
        "type": "git",
        "url": "https://github.com/Trinovantes/rst-compiler"
    },
    "author": {
        "name": "Stephen",
        "email": "hello@stephenli.ca",
        "url": "https://www.stephenli.ca"
    },
    "scripts": {
        "demoDev": "    vite         --config vite.config.demo.ts",
        "demoBuild": "  vite build   --config vite.config.demo.ts",
        "demoPreview": "vite preview --config vite.config.demo.ts",
        "clean": "rm -rf ./dist ./demo/dist",
        "build": "vite build",
        "prepublishOnly": "pnpm clean && pnpm build",
        "lint": "vue-tsc && eslint",
        "test": "vitest --typecheck",
        "playPy": "python3 ./tests/playground/run.py",
        "playJs": "node --import ./build/rawImportHook.ts ./tests/playground/run.ts",
        "playServer": "node --import ./build/rawImportHook.ts ./tests/playground/server.ts",
        "benchmark": "hyperfine --warmup 10 'pnpm playJs' 'pnpm playPy'",
        "cloc": "cloc --vcs=git"
    },
    "dependencies": {
        "katex": "^0.16.33",
        "shiki": "^3.23.0"
    },
    "devDependencies": {
        "@eslint/compat": "^2.0.2",
        "@eslint/js": "^10.0.1",
        "@nuxt/devalue": "^2.0.2",
        "@quasar/extras": "^1.17.0",
        "@quasar/vite-plugin": "^1.10.0",
        "@stylistic/eslint-plugin": "^5.9.0",
        "@types/katex": "^0.16.8",
        "@types/lodash.debounce": "^4.0.9",
        "@types/lodash.merge": "^4.6.9",
        "@types/node": "^25.3.2",
        "@vitejs/plugin-vue": "^6.0.4",
        "date-fns": "^4.1.0",
        "eslint": "^10.0.2",
        "eslint-plugin-n": "^17.24.0",
        "eslint-plugin-vue": "^10.8.0",
        "globals": "^17.3.0",
        "lodash.debounce": "^4.0.8",
        "lodash.merge": "^4.6.2",
        "lz-string": "^1.5.0",
        "mitt": "^3.0.1",
        "monaco-editor": "^0.55.1",
        "quasar": "^2.18.6",
        "sass": "^1.97.3",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.56.1",
        "vite": "^7.3.1",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^4.0.18",
        "vue": "^3.5.29",
        "vue-eslint-parser": "^10.4.0",
        "vue-tsc": "^3.2.5"
    }
}
